<h1>Listing students</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @students.each do |student| %>
  <tr>
    <td><%= student.name %></td>
    <td><%= student.age %></td>
    <td><%= link_to 'Show', school_student_path(@school, student) %></td>
    <td><%= link_to 'Edit', edit_school_student_path(@school, student) %></td>
  </tr>
<% end %>
</table>

